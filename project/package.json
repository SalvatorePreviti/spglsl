{
  "name": "spglsl-project",
  "version": "0.2.0",
  "private": true,
  "description": "spglsl",
  "keywords": [
    "spglsl"
  ],
  "license": "MIT",
  "author": "Salvatore Previti",
  "engines": {
    "node": ">=16.6.1"
  },
  "main": "index.js",
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "test": "mocha --recursive --require @swc-node/register \"test/**/*.test.ts\"",
    "conformance-test-server": "node -r @swc-node/register ./conformance/conformance-test-server-start.ts",
    "conformance-tests": "node -r @swc-node/register ./conformance/conformance-test-runner.ts",
    "precommit": "lint-staged && pretty-quick --staged && ./scripts/build-ts.sh"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx,mts,cts,cjs,mjs,json}": [
      "eslint --no-error-on-unmatched-pattern --fix",
      "prettier --write --loglevel=warn"
    ],
    "*.{yml,yaml,md,htm,html,css,scss,less}": [
      "prettier --write --loglevel=warn"
    ]
  },
  "devDependencies": {
    "@balsamic/eslint-config": "^0.3.0",
    "@swc-node/register": "^1.5.5",
    "@swc/helpers": "^0.4.14",
    "@types/chai": "^4.3.4",
    "@types/cheerio": "^0.22.31",
    "@types/express": "^4.17.15",
    "@types/mocha": "^10.0.1",
    "@types/node": "^18.11.18",
    "@types/puppeteer": "^7.0.4",
    "@typescript-eslint/eslint-plugin": "^5.48.1",
    "@typescript-eslint/parser": "^5.48.1",
    "chai": "^4.3.7",
    "cheerio": "1.0.0-rc.12",
    "eslint": "^8.32.0",
    "eslint-plugin-chai-expect": "^3.0.0",
    "eslint-plugin-json": "^3.1.0",
    "eslint-plugin-mocha": "^10.1.0",
    "eslint-plugin-node": "^11.1.0",
    "express": "^4.18.2",
    "fast-glob": "^3.2.12",
    "husky": "^8.0.3",
    "lint-staged": "^13.1.0",
    "mocha": "^10.2.0",
    "prettier": "^2.8.3",
    "pretty-quick": "^3.1.3",
    "puppeteer": "^19.5.2",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.4"
  }
}
