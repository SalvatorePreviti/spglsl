{
  "C_Cpp.default.configurationProvider": "ms-vscode.cmake-tools",
  "[json]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "editor.formatOnSave": true,
    "editor.quickSuggestions": {
      "comments": true,
      "other": true,
      "strings": true
    }
  },
  "clang-tidy.compilerArgs": [
    "-I../emsdk/upstream/emscripten/system",
    "-I../emsdk/upstream/emscripten/system/include",
    "-I../emsdk/upstream/emscripten/system/include/libcxx",
    "-I../emsdk/upstream/emscripten/system/include/libc",
    "-I../emsdk/upstream/emscripten/system/lib/libc/musl/arch/emscripten",
    "-I../emsdk/upstream/emscripten/system/include/compat"
  ],
  "[jsonc]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "editor.formatOnSave": true,
    "editor.quickSuggestions": {
      "comments": true,
      "other": true,
      "strings": true
    }
  },
  "[html]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "editor.formatOnSave": true
  },
  "[javascript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "editor.formatOnSave": true
  },
  "[javascriptreact]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "editor.formatOnSave": true
  },
  "[typescript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "editor.formatOnSave": true
  },
  "[typescriptreact]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "editor.formatOnSave": true
  },
  "editor.codeActionsOnSave": {
    "source.fixAll": true,
    "source.fixAll.eslint": true
  },
  "editor.detectIndentation": false,
  "editor.formatOnSave": true,
  "editor.lineNumbers": "on",
  "editor.tabSize": 2,
  "eslint.codeActionsOnSave.mode": "all",
  "eslint.format.enable": true,
  "eslint.lintTask.enable": true,
  "eslint.validate": [],
  "javascript.format.enable": true,
  "javascript.format.semicolons": "remove",
  "typescript.format.enable": true,
  "typescript.format.semicolons": "remove",
  "files.eol": "\n",
  "files.exclude": {
    "**/.git": true,
    "**/.svn": true,
    "**/.hg": true,
    "**/CVS": true,
    "**/.DS_Store": true,
    "**/.eslintcache": true,
    "**/.prettiercache": true,
    "**/.cache": true,
    "**/.fuse_hidden*": true
  },
  "search.exclude": {
    "**/node_modules": true,
    "**/bower_components": true,
    "**/package-lock.json": true,
    "**/yarn.lock": true,
    "**/.fusebox/**/*": true,
    "**/.webpack": true,
    "**/.dynamodb/**/*": true,
    "**/*.tsbuildinfo": true,
    "**/.yarn-integrity": true,
    "**/.grunt": true
  },
  "C_Cpp.errorSquiggles": "Enabled",
  "C_Cpp.codeAnalysis.clangTidy.enabled": true,
  "files.associations": {
    "__node_handle": "cpp",
    "__locale": "cpp",
    "ios": "cpp",
    "queue": "cpp",
    "stack": "cpp",
    "vector": "cpp"
  }
}
