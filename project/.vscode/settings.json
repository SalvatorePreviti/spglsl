{
  "C_Cpp.default.configurationProvider": "ms-vscode.cmake-tools",
  "[json]": {
    "editor.formatOnSave": true,
    "editor.quickSuggestions": {
      "comments": true,
      "other": true,
      "strings": true
    }
  },
  "clang-tidy.compilerArgs": [
    "-I../emsdk/upstream/emscripten/system",
    "-I../emsdk/upstream/emscripten/system/include",
    "-I../emsdk/upstream/emscripten/system/include/libcxx",
    "-I../emsdk/upstream/emscripten/system/include/libc",
    "-I../emsdk/upstream/emscripten/system/lib/libc/musl/arch/emscripten",
    "-I../emsdk/upstream/emscripten/system/include/compat"
  ],
  "editor.codeActionsOnSave": {
    "source.fixAll": true,
    "source.fixAll.eslint": true
  },
  "editor.detectIndentation": false,
  "editor.formatOnSave": true,
  "editor.lineNumbers": "on",
  "editor.tabSize": 2,
  "eslint.codeActionsOnSave.mode": "all",
  "eslint.enable": true,
  "eslint.format.enable": true,
  "eslint.lintTask.enable": true,
  "files.eol": "\n",
  "files.exclude": {
    "**/.git": true,
    "**/.svn": true,
    "**/.hg": true,
    "**/CVS": true,
    "**/.DS_Store": true,
    "**/.eslintcache": true,
    "**/.prettiercache": true,
    "**/.cache": true,
    "**/.fuse_hidden*": true
  },
  "javascript.format.enable": true,
  "javascript.format.semicolons": "remove",
  "search.exclude": {
    "emsdk": true,
    "**/node_modules": true,
    "**/bower_components": true,
    "**/package-lock.json": true,
    "**/yarn.lock": true,
    "**/.fusebox/**/*": true,
    "**/.webpack": true,
    "**/.dynamodb/**/*": true,
    "**/*.tsbuildinfo": true,
    "**/.yarn-integrity": true,
    "**/.grunt": true
  },
  "typescript.format.enable": true,
  "typescript.format.semicolons": "remove",
  "eslint.validate": [],
  "files.associations": {
    "iostream": "cpp",
    "streambuf": "cpp",
    "ios": "cpp",
    "__locale": "cpp",
    "__config": "cpp",
    "string": "cpp",
    "string_view": "cpp",
    "iterator": "cpp",
    "iosfwd": "cpp",
    "__string": "cpp",
    "cstdio": "cpp",
    "algorithm": "cpp",
    "cstddef": "cpp",
    "memory": "cpp",
    "__functional_base": "cpp",
    "__bit_reference": "cpp",
    "__debug": "cpp",
    "__errc": "cpp",
    "__hash_table": "cpp",
    "__mutex_base": "cpp",
    "__node_handle": "cpp",
    "__nullptr": "cpp",
    "__split_buffer": "cpp",
    "__sso_allocator": "cpp",
    "__std_stream": "cpp",
    "__threading_support": "cpp",
    "__tree": "cpp",
    "__tuple": "cpp",
    "any": "cpp",
    "array": "cpp",
    "atomic": "cpp",
    "bit": "cpp",
    "bitset": "cpp",
    "cctype": "cpp",
    "charconv": "cpp",
    "chrono": "cpp",
    "cinttypes": "cpp",
    "clocale": "cpp",
    "cmath": "cpp",
    "codecvt": "cpp",
    "complex": "cpp",
    "condition_variable": "cpp",
    "cstdarg": "cpp",
    "cstdint": "cpp",
    "cstdlib": "cpp",
    "cstring": "cpp",
    "ctime": "cpp",
    "cwchar": "cpp",
    "cwctype": "cpp",
    "deque": "cpp",
    "exception": "cpp",
    "fstream": "cpp",
    "functional": "cpp",
    "future": "cpp",
    "initializer_list": "cpp",
    "iomanip": "cpp",
    "istream": "cpp",
    "limits": "cpp",
    "list": "cpp",
    "locale": "cpp",
    "map": "cpp",
    "mutex": "cpp",
    "new": "cpp",
    "numeric": "cpp",
    "optional": "cpp",
    "ostream": "cpp",
    "queue": "cpp",
    "random": "cpp",
    "ratio": "cpp",
    "regex": "cpp",
    "set": "cpp",
    "shared_mutex": "cpp",
    "sstream": "cpp",
    "stack": "cpp",
    "stdexcept": "cpp",
    "strstream": "cpp",
    "system_error": "cpp",
    "thread": "cpp",
    "tuple": "cpp",
    "type_traits": "cpp",
    "typeinfo": "cpp",
    "unordered_map": "cpp",
    "unordered_set": "cpp",
    "utility": "cpp",
    "valarray": "cpp",
    "variant": "cpp",
    "vector": "cpp",
    "*.ipp": "cpp"
  },
  "C_Cpp.errorSquiggles": "Enabled"
}
